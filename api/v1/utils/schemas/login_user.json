{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "login_user.json",
  "type": "object",

  "properties": {
    "email": {
      "type": "string",
      "format": "email", 
    },
    "password": {
      "type": "string",
      "format": "password",
      "maxLength": 255
    }
  },
  "additionalProperties": false,
  "required":  ["email", "password"]
} 
